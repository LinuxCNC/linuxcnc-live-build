#!/bin/sh
curl -sS https://repository.qtpyvcp.com/repo/kcjengr.key | gpg --dearmor | sudo tee /usr/share/keyrings/kcjengr.gpg
sudo gpg --homedir /tmp --no-default-keyring --keyring /etc/apt/keyrings/QTPYVCP.gpg --keyserver keys.openpgp.org --recv-keys 2DEC041F290DF85A
sudo echo "deb [arch=amd64 signed-by=/usr/share/keyrings/QTPYVCP.gpg] https://repository.qtpyvcp.com/apt develop main " | sudo tee -a /etc/apt/sources.list.d/qtpyvcp.list >/dev/null
